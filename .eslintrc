{
    "plugins": [
        "react"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "env": {
        "es6":     true,
        "browser": true,
        "node":    true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended",
        "airbnb"
    ],
    "settings": {
      "react": {
        "version": "16.3"
      }
    },
    "rules": {
        "react/require-default-props": "off",
        "react/forbid-prop-types": "off",
        "no-nested-ternary": "off",
        "react/jsx-filename-extension": "off"
    }
}